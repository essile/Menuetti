@model List<Recipes>
@{
    ViewData["Title"] = "Omni";
}

<h2>Sekaruokavalio</h2>
<p>Jätä haluamasi vaihtoehdot näkyville. Valitsemalla <img src="~/images/delete.png" /> voit jättää rivin tyhjäksi.</p>

@*First carousel*@
@*/*first recipe id from the randomly generated list*/*@
<div id="myCarousel0" class="carousel slide" style="height:70%" data-ride="carousel" data-interval="false">
    <div class="carousel-inner" @*role="listbox" *@>
        <div class="item active" style="height: 70px; text-align:center">
            <p class="btntext" style="text-align: center; padding: 1em 0">
                <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
                <script src="~/js/site.js"></script>
                <script>
/*setting the currently visible recipe id to local storage for later shopping list creation*/
                    localStorage.setItem('recipe0', '@Model[0].RecipeId')
                </script>
                @*showing the currently visible recipe name to user + calling on click to view the recipe details*@
                <a role="button" onclick="showDetCarousel(@Model[0].RecipeId)"> @{await Html.RenderPartialAsync("_RecipeName", Model[0]);} </a>
                <a class="left carousel-control" href="#myCarousel0" role="button" data-slide="prev" onclick="fnSaveChoiceBack()">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel0" role="button" data-slide="next" onclick="fnSaveChoice()">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
                <script>
 /*setting the next recipe id to local storage for later shopping list creation and hiding recipe details*/
                    function fnSaveChoice(){
                        localStorage.setItem('recipe0', '@Model[1].RecipeId')
                        $(".DetCarousel").hide();
                    }
                </script>
            </p>
        </div>
        <h5>@{ await Html.RenderPartialAsync("_DetailsCarousel", Model[0]);}</h5>


        <div class="item" style="height: 70px; text-align:center">
            <p class="btntext" style="text-align: center; padding: 1em 0">
                <a role="button" onclick="showDetCarousel1(@Model[1].RecipeId)"> @{await Html.RenderPartialAsync("_RecipeName", Model[1]);} </a>
                <a class="left carousel-control" href="#myCarousel0" role="button" data-slide="prev" onclick="fnSaveChoiceBack1()">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <script>
                    function fnSaveChoiceBack1() {
                        localStorage.setItem('recipe0', '@Model[0].RecipeId')
                        $(".DetCarousel").hide();
                    }
                </script>
                <a class="right carousel-control" href="#myCarousel0" role="button" data-slide="next" onclick="fnSaveChoice1()">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
                <script>
                    function fnSaveChoice1() {
                        localStorage.setItem('recipe0', '@Model[2].RecipeId')
                        $(".DetCarousel").hide();
                    }
                </script>
            </p>
        </div>
        <h5>@{ await Html.RenderPartialAsync("_DetailsCarousel", Model[1]);}</h5>

        <div class="item" style="height: 70px; text-align:center">
            <p class="btntext" style="text-align: center; padding: 1em 0">
                <a role="button" onclick="showDetCarousel2(@Model[2].RecipeId)"> @{await Html.RenderPartialAsync("_RecipeName", Model[2]);}  </a>
                <a class="left carousel-control" href="#myCarousel0" role="button" data-slide="prev" onclick="fnSaveChoiceBack2()">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <script>
                    function fnSaveChoiceBack2() {
                        localStorage.setItem('recipe0', '@Model[1].RecipeId')

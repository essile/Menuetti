                <a class="right carousel-control" href="#myCarousel0" role="button" data-slide="next" onclick="fnSaveChoice2()">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
                <script>
                    function fnSaveChoice2() {
                        localStorage.setItem('recipe0', 0)
                        $(".DetCarousel").hide();
                    }
                </script>
            </p>
        </div>
        <h5>@{ await Html.RenderPartialAsync("_DetailsCarousel", Model[2]);}</h5>

        @*last item in carousel with no recipe, leaving it blank*@
        <div class="item" style="height: 70px; text-align:center">
            <p class="btntext" style="text-align: center; padding: 1em 0">
                <img src="~/images/delete.png" />
